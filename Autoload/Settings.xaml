<UserControl x:Class="Autoload.Settings"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="250" d:DesignWidth="350">
    <Grid>
        <ListBox Name="lbPlugins" ItemsSource="{Binding Path=Plugins}" Height="194" HorizontalAlignment="Left" Margin="12,12,0,0" VerticalAlignment="Top" Width="326" />
        <Button Content="Add" Height="23" HorizontalAlignment="Left" Margin="12,212,0,0" Name="btnAdd" VerticalAlignment="Top" Width="75" Click="btnAdd_Click" />
        <Button Content="Remove" Height="23" HorizontalAlignment="Left" Margin="93,212,0,0" Name="btnRemove" VerticalAlignment="Top" Width="75" Click="btnRemove_Click" />
    </Grid>
</UserControl>
